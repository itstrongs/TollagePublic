<%@ page language="java" import="java.util.*" pageEncoding="utf-8"%><%@ taglib uri = "http://java.sun.com/jstl/core_rt" prefix="c" %><%String path = request.getContextPath();String basePath = request.getScheme()+"://"+request.getServerName()+":"+request.getServerPort()+path+"/";%><!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"><html>  <head>    <base href="<%=basePath%>">        <title>办税公开后台系统</title>	<meta http-equiv="pragma" content="no-cache">	<meta http-equiv="cache-control" content="no-cache">	<meta http-equiv="expires" content="0">    	<meta http-equiv="keywords" content="keyword1,keyword2,keyword3">	<meta http-equiv="description" content="This is my page">	<!--	<link rel="stylesheet" type="text/css" href="styles.css">	-->		<style type="text/css">		body{ text-align:center }		a{text-decoration : none}		div{ margin:0 auto;  background:url(images/tollage_bg.jpg) no-repeat 4px 5px; background-size:100%; }		.menu{ color:#FFFFFF; font-size:16px; padding:6px; display:inline-block; width:300px; background:#436EEE }		.title{ color:#FFFFFF; font-size:28px; }	</style>  </head>    <body>  	<br><br>    <div style="width: 450px; height:800px;" align="center">    	<br>		<img src="images/logo.png"/>		<a href="tollage/home" class="title">宜丰县国地税政务公开</a>		<br><br>		<input type="button" value="+" onclick="location='tollage/add?key=-1'"/><br>		<c:forEach items="${menu_list}" var="menu">			<input type="button" value="x" onclick="location='tollage/delete?id=${menu.id }&key=${menu.key }'"/>			<a href="tollage/home?key=${menu.id }&ext=${menu.extension }&name=${menu.name }"><label class="menu">${menu.name }</label></a>			<input type="button" value="+" onclick="location='tollage/add?key=${menu.key }'"/>			<br><br>		</c:forEach>	</div>  </body></html>
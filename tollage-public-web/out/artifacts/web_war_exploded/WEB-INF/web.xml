<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_3_1.xsd"
           version="3.1">

    <servlet>
        <servlet-name>IMusicServlet</servlet-name>
        <servlet-class>com.itstrongs.imusic.IMusicServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>IMusicServlet</servlet-name>
        <url-pattern>/imusic/*</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>TollageServlet</servlet-name>
        <servlet-class>com.itstrongs.tollage.TollageServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>TollageServlet</servlet-name>
        <url-pattern>/tollage/*</url-pattern>
    </servlet-mapping>
</web-app>
